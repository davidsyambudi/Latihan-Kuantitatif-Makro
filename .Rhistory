library(WDI)
# GDP riil Indonesia (kode indikator: NY.GDP.MKTP.KD)
gdp_idn <- WDI(country = "IDN",
indicator = "NY.GDP.MKTP.KD",
start = 2000, end = 2023)
head(gdp_idn)
View(gdp_idn)
View(gdp_idn)
# install.packages("WDI") # jalankan sekali di Console bila belum terpasang
library(WDI)
# Mengambil seluruh deret waktu GDP Indonesia (NY.GDP.MKTP.KD)
gdp_idn <- WDI(
country   = "IDN",
indicator = "NY.GDP.MKTP.KD"
)
# Mengurutkan dari tahun paling lama ke tahun terbaru
gdp_idn <- gdp_idn[order(gdp_idn$year), ]
# Melihat beberapa baris awal dan terakhir
head(gdp_idn)
tail(gdp_idn)
# Mengecek struktur data
str(gdp_idn)
plot(
gdp_idn$year,
gdp_idn$NY.GDP.MKTP.KD,
type = "l",
xlab = "Tahun",
ylab = "GDP (constant 2015 US$)",
main = "Perkembangan GDP Indonesia (konstan 2015 US$)"
)
